{
    "version": 2,
    "builds": [
        {
            "src": "dcrm/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize":"15mb", "runtime":"python3.11"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {"dixtDir":"staticfiles_build"}
        }

    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dist": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dist": "dcrm/wsgi.py"
        }
    ]

}